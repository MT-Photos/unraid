<?xml version="1.0"?>
<Container version="2">
    <Name>MtPhotos</Name>
    <Repository>mtphotos/mt-photos:latest</Repository>
    <Registry>https://hub.docker.com/r/mtphotos/mt-photos</Registry>
    <Network>bridge</Network>
    <MyIP/>
    <Shell>sh</Shell>
    <Privileged>false</Privileged>
    <Support>https://mtmt.tech/contact</Support>
    <Project>https://mtmt.tech</Project>
    <Overview>MT Photos is a photo management system made for Nas users.&#xD;
        Through AI technology, automatically organize and classify your photos, including but not limited to time, location, person, and photo type.</Overview>
    <Category>Productivity: MediaApp:Photos</Category>
    <WebUI>http://[IP]:[PORT:8063]/</WebUI>
    <TemplateURL/>
    <Icon>https://cdn.mtmt.tech/logo-r.png</Icon>
    <ExtraParams/>
    <PostArgs/>
    <CPUset/>
    <DateInstalled>1682047314</DateInstalled>
    <DonateText/>
    <DonateLink/>
    <Requires/>
    <Config Name="Web Service Port" Target="8063" Default="8063" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="null">8063</Config>
    <Config Name="Data storage directory" Target="/config" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="null"/>
    <Config Name="App backup directory" Target="/upload" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="null"/>
</Container>